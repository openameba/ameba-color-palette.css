{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:linters",
    ":automergeAll",
    ":label(renovate)",
    ":preserveSemverRanges",
    ":timezone(Asia/Tokyo)",
    "helpers:pinGitHubActionDigestsToSemver"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "reviewers": [
    "team:spindle-working-group"
  ]
}
